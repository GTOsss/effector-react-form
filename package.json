{
  "name": "effector-react-form",
  "version": "2.0.20-canary-1",
  "description": "form manager",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs.js",
      "default": "./lib/index.esm.js"
    },
    "./ssr": {
      "require": "./lib/ssr/index.cjs.js",
      "default": "./lib/ssr/index.esm.js"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:scr && npm run build:ssr",
    "prebuild": "cross-env rimraf lib",
    "build:scr": "cross-env tsc --outDir ./lib/ && NODE_ENV=scr rollup -c",
    "build:ssr": "cross-env tsc --outDir ./lib/ssr && NODE_ENV=ssr rollup -c",
    "pretty": "cross-env prettier --write '**/*.{j,t}{s,sx}'",
    "lint": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "test": "cross-env jest",
    "commit": "cross-env git-cz"
  },
  "author": "Goncharov Timofey",
  "repository": {
    "type": "git",
    "url": "https://github.com/GTOsss/effector-react-form"
  },
  "license": "ISC",
  "peerDependencies": {
    "effector": "^21.6.0",
    "effector-react": "^21.0.8",
    "lodash.topath": "^4.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "all-contributors-cli": "^6.19.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "effector": "^21.6.1",
    "effector-react": "^21.0.8",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "lodash.topath": "^4.5.2",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
